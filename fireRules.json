{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
      "$contact": {
          "name": {
              ".validate": "newData.isString() && newData.val().length >= 2 && newData.val().length <= 48"
          },
          "businessId": {
              ".validate": "newData.isNumber() && newData.val() > 99999999"
          },
          "address": {
              ".validate": "newData.val().length < 50"
          }
      }
    }
  }
}